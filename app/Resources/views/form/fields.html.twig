{# app/Resources/views/form/fields.html.twig #}
{% block form_row %}
    {% spaceless %}
        <div class="container">
            <div class="row">
                <div class="col-md-1"></div>
                <div class="col-md-10">
                    <div class="col-md-4"></div>
                    <div class="col-md-4 ">
                        <div class="form-group">


                            {{ form_label(form) }}
                            {{ form_errors(form) }}
                            {{ form_widget(form) }}
                        </div>

                    </div>
                    <div class="col-md-4"></div>
                </div>
                <div class="col-md-1"></div>
            </div>
        </div>
    {% endspaceless %}
{% endblock form_row %}